{
    "name": "qr-generator/php-version",
    "description": "QR Code Generator PHP Application",
    "type": "project",
    "require": {
        "php": "^8.0",
        "chillerlan/php-qrcode": "^4.3",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
}